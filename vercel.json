{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "app/**/*.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "actions/**/*.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "regions": ["iad1"],
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "env": {
    "NODE_ENV": "production"
  },
  "headers": [
    {
      "source": "/uploads/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
